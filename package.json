{
  "name": "react-native-shop",
  "version": "1.7.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start -c",
    "start:local": "cross-env EXPO_NO_DOTENV=1 DOTENV_FILE=.env.local expo start -c",
    "start:localDebug": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.local.debug expo start -c",
    "start:uat:debug": "cross-env EXPO_NO_DOTENV=1 DOTENV_FILE=.env.uat.debug expo start -c",
    "start:prod": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.prod expo start -c",
    "config:localDebug": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.local.debug expo config --type public",
    "config:uat": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.uat expo config --type public",
    "config:prod": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.prod expo config --type public",
    "start:uat": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.uat expo start -c",
    "android": "expo run:android",
    "android:local": "dotenv -e .env.local expo run:android",
    "android:uat": "dotenv -e .env.uat expo run:android",
    "android:prod": "dotenv -e .env.prod expo run:android",
    "ios": "expo run:ios",
    "ios:local": "dotenv -e .env.local expo run:ios",
    "ios:uat": "dotenv -e .env.uat expo run:ios",
    "ios:prod": "dotenv -e .env.prod expo run:ios",
    "web": "expo start --web -c",
    "web:uat": "expo start --web --env-file .env.uat",
    "web:prod": "expo start --web --env-file .env.prod",
    "update:uat": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.uat dotenv -e .env.uat -o -- eas update --branch develop --message \"uat v$(node -p \\\"require('./package.json').version\\\")\"",
    "update:prod": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.prod dotenv -e .env.prod -o -- eas update --branch master --message \"prod v$(node -p \\\"require('./package.json').version\\\")\"",
    "build:uat:android": "cross-env DOTENV_FILE=.env.uat eas build --profile uat --platform android",
    "build:prod:android": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.prod eas build --profile production --platform android",
    "build:uat:ios": "cross-env DOTENV_FILE=.env.uat eas build --profile uat --platform ios",
    "build:prod:ios": "EXPO_NO_DOTENV=1 DOTENV_FILE=.env.prod eas build --profile production --platform ios",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release:patch": "git branch --show-current | grep -q master && npm version patch && git push --follow-tags || echo \"❌ Switch to master before releasing\"",
    "release:minor": "git branch --show-current | grep -q master && npm version minor && git push --follow-tags || echo \"❌ Switch to master before releasing\"",
    "release:major": "git branch --show-current | grep -q master && npm version major && git push --follow-tags || echo \"❌ Switch to master before releasing\"",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint 'app/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@remotemerge/nepali-date-converter": "^1.0.1",
    "@tanstack/react-query": "^5.67.1",
    "axios": "^1.7.9",
    "baseline-browser-mapping": "^2.9.19",
    "bikram-sambat-js": "^1.0.3",
    "caniuse-lite": "^1.0.30001766",
    "dotenv": "^17.2.3",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.9",
    "expo-constants": "^18.0.13",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-modules-core": "~3.0.21",
    "expo-updates": "~29.0.12",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.7.2",
    "nativewind": "^4.1.23",
    "qs": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-config": "^1.5.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-swipeable-item": "^2.0.9",
    "react-native-user-inactivity": "^1.2.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.1.5",
    "@types/react-native": "^0.73.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-redux": "^7.1.34",
    "autoprefixer": "^10.4.20",
    "babel-preset-expo": "~13.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-native": "^5.0.0",
    "metro-react-native-babel-transformer": "^0.77.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "@babel/core": "^7.21.0"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
