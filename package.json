{
  "name": "react-native-shop",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start -c",
    "start:local": "dotenv -e .env.local expo start -c",
    "start:uat": "dotenv -e .env.uat expo start -c",
    "start:prod": "dotenv -e .env.prod expo start -c",
    "android": "expo run:android",
    "android:local": "dotenv -e .env.local expo run:android",
    "android:uat": "dotenv -e .env.uat expo run:android",
    "android:prod": "dotenv -e .env.prod expo run:android",
    "ios": "expo run:ios",
    "ios:local": "dotenv -e .env.local expo run:ios",
    "ios:uat": "dotenv -e .env.uat expo run:ios",
    "ios:prod": "dotenv -e .env.prod expo run:ios",
    "web": "expo start --web -c",
    "web:uat": "expo start --web --env-file .env.uat",
    "web:prod": "expo start --web --env-file .env.prod",
    "build:local:android": "eas build --profile local      --platform android",
    "build:uat:android": "eas build --profile uat        --platform android",
    "build:prod:android": "eas build --profile production --platform android",
    "build:uat:ios": "eas build --profile uat        --platform ios",
    "build:prod:ios": "eas build --profile production --platform ios",
    "update:local": "eas update --branch local",
    "update:uat": "dotenv -e .env.uat -- eas update --branch",
    "update:prod": "eas update --branch production",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint 'app/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tanstack/react-query": "^5.67.1",
    "axios": "^1.7.9",
    "expo": "^52.0.25",
    "expo-updates": "~0.27.4",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "nativewind": "4.0.1",
    "qs": "^6.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-config": "^1.5.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.11.2",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-swipeable-item": "^2.0.9",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "@types/react": "~18.3.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "autoprefixer": "^10.4.20",
    "babel-preset-expo": "^12.0.6",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-native": "^5.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "@babel/core": "^7.21.0"
  },
  "private": true
}
